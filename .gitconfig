[diff]
        tool = vimdiff
[merge]
        tool = vimdiff
[alias]
        df = difftool -y
        st = status
        br = branch
        co = checkout
        cm = commit -m
        lg = log --oneline --graph --name-status
        lgg= log --graph --abbrev-commit --decorate --format=format:'%C(bold blue)%h%C(reset) - %C(bold cyan)%ah%C(reset)     %C(white)%s%C(reset) %C(dim white)- %an%C(reset)  %C(bold green)(%ar)%C(reset) %C(auto)%d%C(reset)'
